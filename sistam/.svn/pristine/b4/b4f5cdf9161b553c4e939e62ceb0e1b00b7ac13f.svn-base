package br.com.petrobras.sistam.common.valueobjects;

import br.com.petrobras.fcorp.common.beans.AbstractBean;
import br.com.petrobras.sistam.common.entity.Pais;
import java.io.Serializable;

/**
 * Filtro de busca para objetos da classe Embarcacao.
 */
public class FiltroEmbarcacao extends AbstractBean implements Serializable {

    private static final long serialVersionUID = 1L;
    private String nomeEmbarcacao;
    private Pais bandeira;
    private String imo;

    public String getNomeEmbarcacao() {
        return nomeEmbarcacao;
    }

    public void setNomeEmbarcacao(String nomeEmbarcacao) {
        this.nomeEmbarcacao = nomeEmbarcacao;
    }

    
    public Pais getBandeira() {
        return bandeira;
    }

    public void setBandeira(Pais bandeira) {
        this.bandeira = bandeira;
    }

    public String getImo() {
        return imo;
    }

    public void setImo(String imo) {
        this.imo = imo;
    }

    

  

   
    
    
}
